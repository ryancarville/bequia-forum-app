{
  "version": 2,
  "name": "bequia-forum",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/mainfest.json",
      "dest": "/mainfest.json",
      "type": "application/json"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
