{
  "version": 2,
  "name": "bequia-forum",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/manifest.json",
      "headers": { "key": "content-type", "value": "application/json" },
      "dest": "/manifest.json"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
